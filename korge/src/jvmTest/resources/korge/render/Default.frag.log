#version 100 compatibility
#ifdef GL_ES
	precision highp float;
	precision highp int;
	precision lowp sampler2D;
	precision lowp samplerCube;
#else
	  #define highp 
	  #define mediump 
	  #define lowp 
#endif
uniform sampler2D u_Tex0;
uniform sampler2D u_Tex1;
uniform sampler2D u_Tex2;
uniform sampler2D u_Tex3;
varying lowp float v_TexIndex;
varying mediump vec2 v_Tex;
varying vec4 v_Col;
void main() {
	if ((v_TexIndex <= 1.0)) {
		if ((v_TexIndex == 0.0)) {
			gl_FragColor = texture2D(u_Tex0, v_Tex.xy);
		}
		else {
			gl_FragColor = texture2D(u_Tex1, v_Tex.xy);
		}
	}
	else {
		if ((v_TexIndex == 2.0)) {
			gl_FragColor = texture2D(u_Tex2, v_Tex.xy);
		}
		else {
			gl_FragColor = texture2D(u_Tex3, v_Tex.xy);
		}
	}
	gl_FragColor = (gl_FragColor * v_Col);
	if ((gl_FragColor.a <= 0.0)) {
		discard;
	}
}
